.header
    display flex
    align-items center
    justify-content space-between
    width 100%
    height 60px
    top 0
    padding 20px 40px
    box-shadow 0 4px 4px rgba(0, 0, 0, 0.1)
    .header-logo
        svg
            width 160px
            height 56px
    .header-links
        display flex
        a
            display flex
            align-items center
            font-size 15px
            margin-right 40px
            font-weight 600
